//Simple pipe line 

pipeline {
    agent any
    stages {
        stage('Checkout') {
            steps {
                https://github.com/umarqazii/mlops-task3.git
            }
        }
        stage('Build') {
            steps {
                sh 'echo "Building the app"'
            }
        }
        stage('Test') {
            steps {
                sh 'echo "Testing the app"'
            }
        }
        stage('Deploy') {
            steps {
                sh 'echo "Deploying the app"'
            }
        }
        
        
    }
}
```